{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "ecmaFeatures": {
    "modules": true,
    "classes": true
  },
  "rules": {
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-alert": [2],
    "no-resv-key": 2,
    "eqeqeq": 2,
    "no-mixed-requires": [0],
    "no-multi-spaces": [2],
    "no-new-wrappers": [2],
    "no-octal": [2],
    "no-sync": [2],
    "camelcase": [2],
    "new-cap": [2],
    "no-new-object": [2],
    "valid-jsdoc": [2],
    "no-multiple-empty-lines": [2],
    "no-underscore-dangle": [0]
  }
}